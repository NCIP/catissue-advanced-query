<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
	<class name="edu.wustl.query.domain.Execution"
		table="EXECUTION" dynamic-update="false" dynamic-insert="false">
		<cache usage="read-write" />

		<id name="id" column="IDENTIFIER" type="long" length="30"
			unsaved-value="null">
			<generator class="assigned">
				<param name="sequence">EXECUTION_SEQ</param>
			</generator>
		</id>

		<many-to-one name="constraints"
			class="edu.wustl.common.querysuite.queryobject.impl.Constraints"
			cascade="none" outer-join="auto" update="true" insert="true"
			column="CONSTRAINTS_ID" unique="true" />

		<list name="outputTerms" cascade="none"
			table="EXECUTION_TO_OUTPUT_TERMS">
			<cache usage="read-write" />
			<key column="EXECUTION_ID" />
			<index column="POSITION" />
			<many-to-many column="OUTPUT_TERM_ID" unique="true"
				class="edu.wustl.common.querysuite.queryobject.impl.OutputTerm" />
		</list>
		
		<!-- <list name="outputAttributeList" table="EXECUTION_TO_OUTPUT_ATTRIBUTE"
				lazy="false" inverse="false" cascade="none">
				<cache usage="read-write" />
				<key column="EXECUTION_ID" />
				<index column="POSITION" type="int" />
				<many-to-many  column="ATTRIBUTE_ID" unique="true"
					class="edu.wustl.common.querysuite.queryobject.impl.OutputAttribute" />
			</list>
		 -->	
			<list name="parameters" cascade="none"
				table="EXECUTION_TO_OUTPUT_PARAMETERS">
				<cache usage="read-write" />
				<key column="EXECUTION_ID" />
				<index column="POSITION" />
				<many-to-many column="PARAMETER_ID" unique="true"
					class="edu.wustl.common.querysuite.queryobject.impl.Parameter" />
			</list>

	</class>

</hibernate-mapping>
