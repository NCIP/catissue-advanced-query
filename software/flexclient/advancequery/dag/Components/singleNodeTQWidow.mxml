        private function  allValidations() : Boolean
        {
        	 //Here first validate if all combo boxes are selected for a value
			var isValid : Boolean = true;
			if(arithmaticOpsCb.selectedIndex == -1 || attributesCb.selectedIndex == -1 || relationalOpsCb.selectedIndex == -1)
			{
				//if  value from any of the combo box is not selected then return
				createBigTip("Please select the values from dropdown boxes.")
				AlertManager.showWarning("Please select the values from dropdown boxes.");
				isValid = false;
			}
			else
			{
				//If  all comboboxes are selected on LHS then 
				//then validate the RHS side
				if(relationalOpsCb.selectedItem.toString() == "Between")
				{
	               isValid = allBetweenValidations();
	            }
	            else
	            {
	            	//This is the case if it's not between relational op
	            	var  existingTextBox : TextInput = myBox.getChildByName("timeInputBox") as TextInput;
	            	if(existingTextBox != null)
	            	{
	            		var isTextInputValid : Boolean = validateTextInput(existingTextBox);
	            		if(isTextInputValid == false)
	            		{
	            			createBigTip("Please enter the value for Time value for Input box.")
	               			AlertManager.showWarning("Please enter the value for Time value for Input box.");
							isValid = false;
	            		}
	            	}
	            }
			}
			
        	return isValid;
        }
		
