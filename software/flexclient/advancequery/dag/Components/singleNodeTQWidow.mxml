        private function allBetweenValidations() : Boolean
        {
        	var isValid : Boolean = true;
			var operand : String  =  attributesCb.selectedItem.toString();
			var nodeList : ArrayCollection = entityDp[operand];
			var operandType : String  = String(nodeList.getItemAt(1));
			var isfirstTextInputValid : Boolean  =  true;
