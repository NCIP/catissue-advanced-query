	             }
	        }
	        else
	        {
	        
	              	 	//This is case when second operand is an Quantative attribute
	              	 	//In this we we will have Date Literals
	              	 	var datePicker1 : DateField = myBox.getChildByName("datePicker1") as DateField;
	              	 	if(datePicker1 != null)
	              	 	{
	              	 	
	              	 	} 
