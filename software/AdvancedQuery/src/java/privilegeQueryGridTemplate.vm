<?xml version="1.0" encoding="iso-8859-1"?>
<rows>
	#foreach( $query in $queryList )
	<row id="$query.queryId">
		<cell><![CDATA[<input name='objCheckbox' value='$query.queryId' type='checkbox'/>]]></cell>	
		<cell>$query.queryId</cell>
		<cell style='font-size:.7em;' title="Description : $query.queryDescription "> $query.queryName^javascript:executeQuery("$query.queryId");^_self</cell>
		<cell>$query.rootEntityName ($query.countOfRootRecords)</cell>
		<cell>$query.executedOn</cell>
		<cell>$query.ownerName </cell>
		#if($queryPageOf == "myQueries")
		<cell><![CDATA[<img src='images/advQuery/application_edit.png' title='Edit Query' onclick='editQuery("$query.queryId")'/>&nbsp&nbsp&nbsp<img src='images/advQuery/execute-button-1.PNG' title='Execute Query' onclick='executeQuery("$query.queryId")'/>&nbsp&nbsp&nbsp<img src='images/advQuery/cancel.png' title='Delete Query' onclick='deleteQueryPopup("$query.queryId")'/>&nbsp&nbsp&nbsp<img src='images/advQuery/export.jpg' title='Export Query' onclick='exportQuery("$query.queryId")'/>]]> </cell>
		#{else}
		<cell><![CDATA[<img src='images/advQuery/application_edit.png' title='Edit Query' onclick='editQuery("$query.queryId")'/>&nbsp&nbsp&nbsp<img src='images/advQuery/execute-button-1.PNG' title='Execute Query' onclick='executeQuery("$query.queryId")'/>&nbsp&nbsp&nbsp<img src='images/advQuery/export.jpg' title='Export Query' onclick='exportQuery("$query.queryId")'/>]]> </cell>
		#end
	</row> 
	#end
</rows>
