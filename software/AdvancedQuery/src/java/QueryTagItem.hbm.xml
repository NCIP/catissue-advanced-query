<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.wustl.common.tags.domain.TagItem" entity-name="QueryTagItem" table="query_tag_items">
		<id name="identifier" type="java.lang.Long" column="identifier">
			<generator class="increment" />
		</id>
		<many-to-one entity-name="QueryTag" name="tag">
			<column name="TAG_ID" not-null="true" />
		</many-to-one>

		<many-to-one name="obj" class="edu.wustl.common.querysuite.queryobject.impl.ParameterizedQuery"	 cascade="none">
		   	<column name="OBJ_ID" not-null="true" />
		</many-to-one>
	</class>
</hibernate-mapping>